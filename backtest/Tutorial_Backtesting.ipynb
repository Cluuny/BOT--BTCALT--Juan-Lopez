    "   \"outputs\": [],\n",
    "   \"source\": [\n",
    "    \"def calculate_rsi(df, period=14):\\n\",\n",
    "    \"    \\\"\\\"\\\"\\n\",\n",
    "    \"    Calcula el RSI (Relative Strength Index)\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    F√≥rmula:\\n\",\n",
    "    \"    1. Calcular cambios de precio (delta)\\n\",\n",
    "    \"    2. Separar ganancias y p√©rdidas\\n\",\n",
    "    \"    3. Calcular promedio de ganancias y p√©rdidas\\n\",\n",
    "    \"    4. RS = Promedio Ganancias / Promedio P√©rdidas\\n\",\n",
    "    \"    5. RSI = 100 - (100 / (1 + RS))\\n\",\n",
    "    \"    \\\"\\\"\\\"\\n\",\n",
    "    \"    df = df.copy()\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    # Cambios de precio\\n\",\n",
    "    \"    delta = df['close'].diff()\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    # Separar ganancias y p√©rdidas\\n\",\n",
    "    \"    gain = delta.where(delta > 0, 0)\\n\",\n",
    "    \"    loss = -delta.where(delta < 0, 0)\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    # Promedios m√≥viles\\n\",\n",
    "    \"    avg_gain = gain.rolling(window=period).mean()\\n\",\n",
    "    \"    avg_loss = loss.rolling(window=period).mean()\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    # Relative Strength\\n\",\n",
    "    \"    rs = avg_gain / avg_loss\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    # RSI\\n\",\n",
    "    \"    df['RSI'] = 100 - (100 / (1 + rs))\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    return df\\n\",\n",
