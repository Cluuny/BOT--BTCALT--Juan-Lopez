    "    \"def calculate_metrics(trades_df, initial_capital):\\n\",\n",
    "    \"    \\\"\\\"\\\"\\n\",\n",
    "    \"    Calcula m√©tricas de performance del backtest\\n\",\n",
    "    \"    \\\"\\\"\\\"\\n\",\n",
    "    \"    if len(trades_df) == 0:\\n\",\n",
    "    \"        print(\\\"‚ö†Ô∏è No hay trades para analizar\\\")\\n\",\n",
    "    \"        return None\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    # M√©tricas b√°sicas\\n\",\n",
    "    \"    total_trades = len(trades_df)\\n\",\n",
    "    \"    winners = trades_df[trades_df['profit'] > 0]\\n\",\n",
    "    \"    losers = trades_df[trades_df['profit'] < 0]\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    win_rate = (len(winners) / total_trades * 100) if total_trades > 0 else 0\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    total_profit = trades_df['profit'].sum()\\n\",\n",
    "    \"    total_return_pct = (total_profit / initial_capital) * 100\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    avg_profit = trades_df['profit'].mean()\\n\",\n",
    "    \"    avg_profit_pct = trades_df['profit_pct'].mean()\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    # Profit factor\\n\",\n",
    "    \"    gross_profit = winners['profit'].sum() if len(winners) > 0 else 0\\n\",\n",
    "    \"    gross_loss = abs(losers['profit'].sum()) if len(losers) > 0 else 0\\n\",\n",
    "    \"    profit_factor = gross_profit / gross_loss if gross_loss > 0 else float('inf')\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    # Sharpe Ratio (simplificado)\\n\",\n",
    "    \"    returns = trades_df['profit_pct']\\n\",\n",
    "    \"    sharpe = (returns.mean() / returns.std()) if returns.std() > 0 else 0\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    # Max Drawdown\\n\",\n",
    "    \"    equity = trades_df['profit'].cumsum() + initial_capital\\n\",\n",
    "    \"    running_max = equity.cummax()\\n\",\n",
    "    \"    drawdown = (equity - running_max) / running_max * 100\\n\",\n",
    "    \"    max_drawdown = drawdown.min()\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    # Duraci√≥n promedio\\n\",\n",
    "    \"    avg_duration_hours = trades_df['duration'].mean()\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    metrics = {\\n\",\n",
    "    \"        'Total Trades': total_trades,\\n\",\n",
    "    \"        'Winners': len(winners),\\n\",\n",
    "    \"        'Losers': len(losers),\\n\",\n",
    "    \"        'Win Rate (%)': win_rate,\\n\",\n",
    "    \"        'Total Profit ($)': total_profit,\\n\",\n",
    "    \"        'Total Return (%)': total_return_pct,\\n\",\n",
    "    \"        'Avg Profit per Trade ($)': avg_profit,\\n\",\n",
    "    \"        'Avg Profit per Trade (%)': avg_profit_pct,\\n\",\n",
    "    \"        'Profit Factor': profit_factor,\\n\",\n",
    "    \"        'Sharpe Ratio': sharpe,\\n\",\n",
    "    \"        'Max Drawdown (%)': max_drawdown,\\n\",\n",
    "    \"        'Avg Duration (hours)': avg_duration_hours,\\n\",\n",
    "    \"    }\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    return metrics\\n\",\n",
    "    \"\\n\",\n",
    "    \"# Calcular m√©tricas\\n\",\n",
    "    \"metrics = calculate_metrics(trades_df, backtest.initial_capital)\\n\",\n",
